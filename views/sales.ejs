<!DOCTYPE html>
<html lang="en">

<head>    
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
     
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sales</title>
</head>
<body>

    <div class="card">
        <div class="card-body">
     <h1>Sales List</h1>
<br>
   

 <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Model Name</th>
        <th scope="col">Transactions</th>
        <th scope="col">Inventory</th>
        <th scope="col">Reports</th>
        <th scope="col">Added By</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    
    <tbody>
<% let i=0%>       
   <%  for(const element of data) { %>

      <tr>
        <th scope="row"><%= i+=1 %></th>
        <td><%= element.modelName %></td>
        <td><%= element.transactions %></td>
        <td><%= element.inventory %></td>
        <td><%= element.reports %></td>
        <td><%= element.userName %></td>
        <td><a type="button" href="/sale/sale/edit/<%= element.id %>" class="btn btn-warning">Edit </a>
        </td>
        <td><a type="button" href="/sale/sale/delete/<%= element.id %>" class="btn btn-danger">Delete </a>
        </td>
      </tr>
       <% }  %>
    </tbody>
  </table>

 <a  href="/sale/sale" class="btn btn-primary">Add Sales</a>  

</div>
</div>
</body>
</html>